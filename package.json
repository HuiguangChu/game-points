{
  "name": "point-collection-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "typesafe-actions": "^5.1.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./webpack/webpack.production.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --mode production --config ./webpack/webpack.bundle-analyzer.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config ./webpack/webpack.dev-server.js",
    "eslint": "eslint --parser-options=project:./tsconfig.json --ext '.js,.jsx,.ts,.tsx' src/",
    "lint": "yarn eslint --fix  && yarn stylelint --fix",
    "stylelint": "stylelint src/**/*.scss",
    "test": "jest --config ./jest.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^7.0.3",
    "css-loader": "^1.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.21",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^2.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-scss": "^4.6.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.78.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2",
    "webpack-merge": "^5.7.3"
  }
